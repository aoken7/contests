#include<bits/stdc++.h>
#define all(a) a.begin(), a.end()
#define put(i) cout<<i<<endl
#define rep(i,s,n) for(long long i=s;i<(long long)(n);i++)
using namespace std;
using ll = long long;

int main(){
    ll n, k; 
    cin >> n >> k;
    vector<string> s(n);
    rep(i,0,n) cin >> s[i];

    ll ans = 0;

    for (int bit = 0; bit < (1<<n); ++bit) {
        vector<int> S;
        for (int i = 0; i < n; ++i) {
            if (bit & (1<<i)) { // 列挙に i が含まれるか
                S.push_back(i);
            }
        }

        map<char,ll> mp;
        for (int i = 0; i < (int)S.size(); ++i) {
            for (auto x:s[S[i]]){
                mp[x]++;
            }
            //cout << S[i];
        }
        //cout << endl;

        ll cnt = 0;
        for (auto x:mp){
            //cout << x.first << " " << x.second << "," ;
            if (x.second == k){
                cnt++;
            }
        }
        ans = max(cnt, ans);
    }
    put(ans);
}
