#include<bits/stdc++.h>
#define all(a) a.begin(), a.end()
#define put(i) cout<<i<<endl
#define rep(i,s,n) for(long long i=s;i<(long long)(n);i++)
using namespace std;
using ll = long long;

int main(){
    string s; cin >> s;
    map<char,ll> mp;

    ll up = 0;
    ll down = 0;

    for (auto x: s){
        mp[x]++;
        if (isupper(x)){
            up++;
        }
        if(islower(x)){
            down++;
        }
        if(mp[x] > 1){
            put("No");
            return 0;
        }
    }

    if(up < 1 or down < 1){
        put("No");
        return 0;
    }

    put("Yes");
}
