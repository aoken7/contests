#include <bits/stdc++.h>
#define all(a) a.begin(), a.end()
#define put(i) cout << i << endl
#define rep(i, s, n) for (long long i = s; i < (long long)(n); i++)
using namespace std;
using ll = long long;

int main() {
    ll n, k;
    cin >> n >> k;
    vector<ll> a(n), b(n);
    rep(i, 0, n) cin >> a[i];
    rep(i, 0, n) cin >> b[i];

    ll diff = 0;

    rep(i, 0, n) { diff += abs(a[i] - b[i]); }

    if (diff <= k and (k - diff) % 2 == 0)
        put("Yes");
    else
        put("No");
}
