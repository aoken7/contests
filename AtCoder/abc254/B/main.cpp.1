#include <bits/stdc++.h>
#define all(a) a.begin(), a.end()
#define put(i) cout << i << endl
#define rep(i, s, n) for (long long i = s; i < (long long)(n); i++)
using namespace std;
using ll = long long;

int main() {
    ll n;
    cin >> n;

    int a[31][31];

    rep(i,0,n){
        rep(j,0,i+1){
            if (j == 0 or j==i){
                a[i][j] = 1;
                cout << 1 << " ";
            }else{
                a[i][j] = a[i-1][j-1] + a[i-1][j];
                cout << a[i][j] << " ";
            }
        }
        cout << endl;
    }
}
